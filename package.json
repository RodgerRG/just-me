    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "concurrently": "^7.6.0",
    "eslint-plugin-storybook": "^0.6.8",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4"
  }
